@model List<SocialMedia.Models.FriendshipRequestViewModel>

<div id="userlist" class="container" style="width:100%; height:230px; overflow:scroll;">

    <div id="userlistcontent">
        @foreach (var item in Model)
        {
            if (item.SenderUser.Id == Convert.ToInt32(Session["Id"]))
            {
                <div id="@item.Id" class="row" style="border-top:1px solid #ECF0F1;">
                    <div class="col-md-6 col-sm-6" style="width:50%; margin-top:6px;">
                        <a href="/MyProfile/UserProfile/@item.ReceiverUser.Id"><img src="~/Images/@item.ReceiverUser.ProfilePhoto" alt="user" class="profile-photo-md" style="width:60px;"></a>
                    </div>
                    <div class="col-md-6 col-sm-6" style="width:50%;">
                        <h5>@item.ReceiverUser.Name @item.ReceiverUser.Surname</h5>
                        <h5>@item.ReceiverUser.Province</h5>
                    </div>
                </div>
            }
            else
            {
                <div id="@item.Id" class="row" style="border-top:1px solid #ECF0F1;">
                    <div class="col-md-6 col-sm-6" style="width:50%;  margin-top:6px;">
                        <a href="/MyProfile/UserProfile/@item.SenderUser.Id"><img src="~/Images/@item.SenderUser.ProfilePhoto" alt="user" class="profile-photo-md" style="width:60px;"></a>
                    </div>
                    <div class="col-md-6 col-sm-6" style="width:50%;">
                        <h5>@item.SenderUser.Name @item.SenderUser.Surname</h5>
                        <h5>@item.SenderUser.Province</h5>
                    </div>
                </div>
            }

        }
    </div>
</div>
<div id="loader"><img src="~/Images/loader.gif" width="30" height="10" style="background-color:whitesmoke" /></div>
UlcH: <p id="height"></p>
ulH: <p id="ULH"></p>
ulST: <p id="ULST"></p>





